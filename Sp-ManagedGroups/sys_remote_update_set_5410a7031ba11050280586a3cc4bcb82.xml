		<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sp_widget"&gt;&lt;sp_widget action="INSERT_OR_UPDATE"&gt;&lt;category&gt;custom&lt;/category&gt;&lt;client_script&gt;&lt;![CDATA[function() {
	/* widget controller */
	var c = this;
	c.selectedGroup = null;
	c.selectedUser = null;
	c.baseSelectableUserFilter = 'active=true';
	c.selectableUserFilter = c.baseSelectableUserFilter;

	c.selectGroup = function(group) {
		c.selectedGroup = group;
		c.server.get({
			action: 'selectGroup',
			group_sys_id: group.sys_id
		}).then(function(response) {
			c.data.groupMembers = response.data.groupMembers;
			c.selectableUserFilter = c.baseSelectableUserFilter;
			c.selectableUserFilter += c.data.groupMembers
				.map(function(value) {
				return '^sys_id!=' + value.user_sys_id;
			}).join('');

			c.data.selectableUserFilter = c.selectableUserFilter;
		});
	}

	c.addMember = function(groupId, userId) {
		c.server.get({
			action: 'addMember',
			group_sys_id: groupId,
			user_sys_id: userId
		}).then(function(response) {
			c.selectGroup(c.selectedGroup);
		});
	}

	c.removeMember = function(memberId) {
		c.server.get({
			action: 'removeMember',
			member_sys_id: memberId
		}).then(function(response) {
			c.selectGroup(c.selectedGroup);
		});
	}
}]]&gt;&lt;/client_script&gt;&lt;controller_as&gt;c&lt;/controller_as&gt;&lt;css&gt;.record-picker {&amp;#13;
  a.select2-choice {&amp;#13;
    border-radius: 4px 0 0 4px;&amp;#13;
  }&amp;#13;
&amp;#13;
  span.select2-arrow {&amp;#13;
    border-radius: 0;&amp;#13;
  }&amp;#13;
}&amp;#13;
&amp;#13;
.record-picker-button {&amp;#13;
  padding: 5px 12px;&amp;#13;
}&amp;#13;
&amp;#13;
.member-list {&amp;#13;
  margin-bottom: 0;&amp;#13;
}&lt;/css&gt;&lt;data_table&gt;sp_instance&lt;/data_table&gt;&lt;demo_data/&gt;&lt;description/&gt;&lt;docs/&gt;&lt;field_list&gt;color,glyph,title&lt;/field_list&gt;&lt;has_preview&gt;false&lt;/has_preview&gt;&lt;id&gt;my_managed_groups&lt;/id&gt;&lt;internal&gt;false&lt;/internal&gt;&lt;link&gt;&lt;![CDATA[function link(scope, element, attrs, controller) {  }]]&gt;&lt;/link&gt;&lt;name&gt;My Managed Groups&lt;/name&gt;&lt;option_schema/&gt;&lt;public&gt;false&lt;/public&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[(function() {

	// Initial load
	if(!input) {
		var myGroupsGr = new GlideRecordSecure('sys_user_group');
		myGroupsGr.addQuery('manager', gs.getUserID());
		myGroupsGr.addActiveQuery();
		myGroupsGr.orderBy('name');
		myGroupsGr.query();

		data.groups = [];
		while(myGroupsGr.next()) {
			data.groups.push({
				name: String(myGroupsGr.name),
				sys_id: String(myGroupsGr.sys_id)
			});
		}

		// Empty data properties
		data.groupMembers = [];
		return;
	}

	// Subsequent requests
	var groupMembersGr = new GlideRecordSecure('sys_user_grmember');

	if(input.action === 'selectGroup') {
		groupMembersGr.addQuery('group', input.group_sys_id);
		groupMembersGr.orderBy('user.name');
		groupMembersGr.query();

		data.groupMembers = [];
		while(groupMembersGr.next()){
			data.groupMembers.push({
				sys_id: String(groupMembersGr.sys_id),
				user_sys_id: String(groupMembersGr.user.sys_id),
				user_name: String(groupMembersGr.user.name)
			});
		}
		return;
	}

	if(input.action === 'addMember') {
		// Check if combo exists.
		groupMembersGr.addQuery('group', input.group_sys_id);
		groupMembersGr.addQuery('user', input.user_sys_id);
		groupMembersGr.setLimit(1);
		groupMembersGr.query();

		if(groupMembersGr.getRowCount() &gt; 0){
			gs.addErrorMessage('Member already exists.');
			return;
		}

		// Check if the user exists.
		var userGr = new GlideRecordSecure("sys_user");
		userGr.addQuery('sys_id', input.user_sys_id);
		userGr.setLimit(1);
		userGr.query();

		if(userGr.getRowCount() &lt; 1){
			gs.addErrorMessage('User not found.');
			return;
		}

		// Check if the group exists.
		var groupGr = new GlideRecordSecure("sys_user_group");
		groupGr.addQuery('sys_id', input.group_sys_id);
		groupGr.setLimit(1);
		groupGr.query();

		if(groupGr.getRowCount() &lt; 1){
			gs.addErrorMessage('Group not found.');
			return;
		} else {
			groupGr.next();
			// Check if the user is allowed because we are going to use insecure GR.
			if(groupGr.manager != gs.getUserID() &amp;&amp; !gs.hasRoleExactly('admin')) {
				gs.addErrorMessage('You are not allowed to modify this group.');
				return;
			}
		}

		// Have to use insecure GR because users can't create group membership.
		// Have to use initialize because even admins can't modify a membership GR after create.
		var insecureGr = new GlideRecord('sys_user_grmember');
		insecureGr.initialize();
		insecureGr.user = input.user_sys_id;
		insecureGr.group = input.group_sys_id;
		insecureGr.insert();

		gs.addInfoMessage('Member has been added.');
		return;
	}

	if(input.action === 'removeMember') {
		groupMembersGr.get(input.member_sys_id);
		var deleted = groupMembersGr.deleteRecord();

		if(deleted === true) {
			gs.addInfoMessage('Member has been removed.');
		} else {
			gs.addErrorMessage('Something happened. Member was not removed.');
		}
		return;
	}
})();]]&gt;&lt;/script&gt;&lt;servicenow&gt;false&lt;/servicenow&gt;&lt;sys_class_name&gt;sp_widget&lt;/sys_class_name&gt;&lt;sys_created_by&gt;SchreinerJ&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2020-06-24 17:28:29&lt;/sys_created_on&gt;&lt;sys_customer_update&gt;false&lt;/sys_customer_update&gt;&lt;sys_id&gt;8900686e1b6d1c10280586a3cc4bcb92&lt;/sys_id&gt;&lt;sys_mod_count&gt;201&lt;/sys_mod_count&gt;&lt;sys_name&gt;My Managed Groups&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_replace_on_upgrade&gt;false&lt;/sys_replace_on_upgrade&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sp_widget_8900686e1b6d1c10280586a3cc4bcb92&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;SchreinerJ&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2020-06-26 19:53:20&lt;/sys_updated_on&gt;&lt;template&gt;&lt;![CDATA[&lt;div class="panel panel-{{::options.color}} b" ng-show="c.data.groups"&gt;
  &lt;div class="panel-heading"&gt;
    &lt;h4 class="panel-title"&gt;
      &lt;fa ng-if="::options.glyph.length" name="{{::options.glyph}}" class="m-r-sm"
          /&gt;
      {{::options.title || "My managed groups"}}&lt;/h4&gt;
  &lt;/div&gt;
  &lt;div class="panel-body"&gt;
    &lt;div class="row"&gt;

      &lt;div class="col-md-6"&gt;
        &lt;div class="panel panel-{{::options.color}} b"&gt;
          &lt;div class="panel-heading"&gt;
            &lt;h4 class="panel-title"&gt;${Groups}&lt;/h4&gt;
          &lt;/div&gt;
          &lt;ul class="list-group"&gt;
            &lt;li ng-repeat="group in c.data.groups" class="list-group-item" ng-class="{'active': c.selectedGroup.sys_id === group.sys_id}" ng-click="c.selectGroup(group)"
                &gt;{{ ::group.name }}&lt;/li&gt;
          &lt;/ul&gt;
        &lt;/div&gt;
      &lt;/div&gt;

      &lt;div class="col-md-6"&gt;
        &lt;div class="panel panel-{{::options.color}} b" ng-if="!c.selectedGroup"&gt;
          &lt;ul class="list-group"&gt;
            &lt;li class="list-group-item text-center"
                &gt;${Select a group.}&lt;/li&gt;
          &lt;/ul&gt;
        &lt;/div&gt;

        &lt;div class="panel panel-{{::options.color}} b" ng-if="c.selectedGroup"&gt;

          &lt;div class="panel-heading"&gt;
            &lt;h4 class="panel-title"&gt;{{ c.selectedGroup.name }}&lt;/h4&gt;
          &lt;/div&gt;

          &lt;div class="panel-body"&gt;

            &lt;div class="form-group"&gt;
              &lt;label for="selectedUser"&gt;Add member&lt;/label&gt;

              &lt;div class="input-group"&gt;
                &lt;sn-record-picker id="selectedUser" field="c.selectedUser" table="'sys_user'" default-query="c.selectableUserFilter" value-field="'sys_id'" display-field="'name'" search-fields="'name'" page-size="100" class="record-picker" /&gt;
                &lt;div class="input-group-btn"&gt;
                  &lt;button class="btn btn-primary record-picker-button" id="addMember" ng-click="c.addMember(c.selectedGroup.sys_id, c.selectedUser.value)"
                          &gt;${Add}&lt;/button&gt;
                &lt;/div&gt;
              &lt;/div&gt;
            &lt;/div&gt;

            &lt;div class="panel panel-{{::options.color}} b member-list"&gt;
              &lt;div class="panel-heading"&gt;
                &lt;h4 class="panel-title"&gt;${Members}&lt;/h4&gt;
              &lt;/div&gt;
              &lt;ul class="list-group"&gt;
                &lt;li ng-repeat="member in c.data.groupMembers" class="list-group-item"
                    &gt;{{ ::member.user_name }}
                  &lt;a class="pull-right" id="removeMember" ng-click="c.removeMember(member.sys_id)"&gt;
                    &lt;i class="fa fa-trash" aria-hidden="true"&gt;&lt;/i&gt;
                  &lt;/a&gt;
                &lt;li class="list-group-item text-center" ng-if="c.data.groupMembers.length == 0"
                    &gt;${No members}&lt;/li&gt;
              &lt;/ul&gt;
            &lt;/div&gt;

          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/div&gt;
  &lt;/div&gt;
&lt;/div&gt;
]]&gt;&lt;/template&gt;&lt;/sp_widget&gt;&lt;/record_update&gt;</payload>
